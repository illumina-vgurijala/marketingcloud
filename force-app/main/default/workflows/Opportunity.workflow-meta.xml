    <rules>
        <fullName>Opportunity%3A CRF Section 1 Field Service Engineer Not Available</fullName>
        <actions>
            <name>Opportunity_Send_CRF_Section_1_Completion_Notification_When_Field_Service_Engine</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Opportunity_Set_Bypass_Flag_To_True</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Opportunity_Update_CRF_Sec1_No_FSE_Flag</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Sends email to Opportunity owner when Field Service Engineer Not Available and Customer Readiness section 1 completed field is checked.</description>
        <formula>AND (     RecordType.DeveloperName = $Label.OpportunityRTDirectStandard,     TEXT( StageName ) = $Label.OpportunityStageDevelop,     Is_Customer_Readiness_Section_1_Complete__c,     Are_Products_Added__c,     ISBLANK ( Primary_Field_Service_Engineer__c ),     CONTAINS(Product_Type__c , $Label.OpportunityProdTypeInstrument),     ISBLANK ( Primary_Field_Application_Scientist__c ),     NOT (         OR(             ISBLANK ( Customer_s_Next_Step__c ),             CRF_Sec1_No_FSE_Flag__c                                                      )     ) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
