        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity%3A Update Backwards Movement in Pipeline</fullName>
        <actions>
            <name>Update_Backwards_Movement_in_Pipeline</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>Updates Backwards Movement in Pipeline field on opportunity when the opportunity is moved backward in stage.</description>
        <formula>AND(          OR(                  AND(                          OR(                 RecordType.DeveloperName = $Label.OpportunityRTDirectStandard,                 RecordType.DeveloperName = $Label.OpportunityRTIndirectStandard               ),             OR(                 TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageFinalize,                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageNegotiateResolve,                     NOT(                         TEXT(StageName)==$Label.OpportunityStageFinalize                        )                    ),                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageProve,                     NOT(                         OR(                             TEXT(StageName)==$Label.OpportunityStageFinalize,                             TEXT(StageName)==$Label.OpportunityStageNegotiateResolve                           )                        )                    ),                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageDevelop,                     NOT(                         OR(                             TEXT(StageName)==$Label.OpportunityStageProve,                             TEXT(StageName)==$Label.OpportunityStageNegotiateResolve,                             TEXT(StageName)==$Label.OpportunityStageFinalize                           )                        )                    ),                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageQualify,                     NOT(                         OR(                             TEXT(StageName)==$Label.OpportunityStageDevelop,                             TEXT(StageName)==$Label.OpportunityStageProve,                             TEXT(StageName)==$Label.OpportunityStageNegotiateResolve,                             TEXT(StageName)==$Label.OpportunityStageFinalize                           )                        )                    )               )            ),                 AND(                     OR(                         RecordType.DeveloperName = $Label.OpportunityRTDirectReOrder,                         RecordType.DeveloperName = $Label.IndirectReOrder,                         RecordType.DeveloperName = $Label.FieldServices,                         RecordType.DeveloperName = $Label.IndirectFieldServices                       ),                     OR(                         TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageFinalize,                         AND(                             TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageDevelop,                             NOT(                                 TEXT(StageName)==$Label.OpportunityStageFinalize                                )                            )                       )                    )               ),             ISCHANGED(StageName),             NOT(                 OR(                     TEXT(StageName)==$Label.OpportunityStageClosedWon,                     TEXT(StageName)==$Label.OpportunityStageClosedLost,                     TEXT(StageName)==$Label.OpportunityStageClosedAbandoned                   )                )    )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Opportunity%3A Update Close Date For Closed Opportunity</fullName>
        <actions>
            <name>Opportunity_Update_Close_Date_To_Today</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <booleanFilter>1 OR 2 OR 3</booleanFilter>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Lost</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Abandoned</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Won</value>
        </criteriaItems>
        <description>Updates Close Date on an Opportunity to TODAY() for all the Closed Opportunity Stages.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>

    <rules>
        <fullName>Opportunity%3A Update Opportunity Stage to Closed Won on Close Date</fullName>
        <active>true</active>
        <description>Update Opportunity Stage to Closed Won on Last Date of Quarter</description>
        <formula>AND (      $RecordType.DeveloperName = $Label.OpportunityRTStandingQuote ,      NOT(          OR (             TEXT(StageName) = $Label.OpportunityStageClosedWon,              TEXT(StageName) = $Label.OpportunityStageClosedAbandoned          )      ) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Opportunity_Set_Stage_to_Closed_Won</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Opportunity.Last_Date_Of_Quarter__c</offsetFromField>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>

    <rules>
        <fullName>Opportunity%3AWar Room Notification</fullName>
        <actions>
            <name>Opportunity_Channel_Partner_War_Room_Opportunity</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Update_Recipient_Email</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.Is_Deal_Registration__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.War_Room_Request__c</field>
            <operation>equals</operation>
            <value>Yes</value>
        </criteriaItems>
        <description>Send Notification when Channel Partner Create or Updates an Opportunity as War Room</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Update Backwards Movement in Pipeline</fullName>
        <actions>
            <name>Update_Backwards_Movement_in_Pipeline</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Updates Backwards Movement in Pipeline field on opportunity when the opportunity is moved backward in stage.</description>
        <formula>AND(          OR(                  AND(                          OR(                 RecordType.DeveloperName = $Label.OpportunityRTDirectStandard,                 RecordType.DeveloperName = $Label.OpportunityRTIndirectStandard               ),             OR(                 TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageFinalize,                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageNegotiateResolve,                     NOT(                         TEXT(StageName)==$Label.OpportunityStageFinalize                        )                    ),                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageProve,                     NOT(                         OR(                             TEXT(StageName)==$Label.OpportunityStageFinalize,                             TEXT(StageName)==$Label.OpportunityStageNegotiateResolve                           )                        )                    ),                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageDevelop,                     NOT(                         OR(                             TEXT(StageName)==$Label.OpportunityStageProve,                             TEXT(StageName)==$Label.OpportunityStageNegotiateResolve,                             TEXT(StageName)==$Label.OpportunityStageFinalize                           )                        )                    ),                 AND(                     TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageQualify,                     NOT(                         OR(                             TEXT(StageName)==$Label.OpportunityStageDevelop,                             TEXT(StageName)==$Label.OpportunityStageProve,                             TEXT(StageName)==$Label.OpportunityStageNegotiateResolve,                             TEXT(StageName)==$Label.OpportunityStageFinalize                           )                        )                    )               )            ),                 AND(                     OR(                         RecordType.DeveloperName = $Label.OpportunityRTDirectReOrder,                         RecordType.DeveloperName = $Label.IndirectReOrder,                         RecordType.DeveloperName = $Label.FieldServices,                         RecordType.DeveloperName = $Label.IndirectFieldServices                       ),                     OR(                         TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageFinalize,                         AND(                             TEXT(PRIORVALUE(StageName))==$Label.OpportunityStageDevelop,                             NOT(                                 TEXT(StageName)==$Label.OpportunityStageFinalize                                )                            )                       )                    )               ),             ISCHANGED(StageName),             NOT(                 OR(                     TEXT(StageName)==$Label.OpportunityStageClosedWon,                     TEXT(StageName)==$Label.OpportunityStageClosedLost,                     TEXT(StageName)==$Label.OpportunityStageClosedAbandoned                   )                )    )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
</Workflow>