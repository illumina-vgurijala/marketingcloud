    TEXT([SVMXC__Service_Order__c].SVMXC__Billing_Type__c) = $Label.WorkOrderBillingTypeBillable
   )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
    ISCHANGED({!myVariable_current.SVMXC__Order_Status__c}),
    PRIORVALUE({!myVariable_current.SVMXC__Order_Status__c})&lt;&gt;{!$Label.WorkOrderStatusCompleted},
    TEXT({!myVariable_current.SVMXC__Order_Status__c})= {!$Label.WorkOrderStatusCompleted},
OR(
    TEXT({!myVariable_current.SVMXC__Order_Type__c}) = {!$Label.WorkOrderTypeInstallation},
    TEXT({!myVariable_current.SVMXC__Billing_Type__c}) = {!$Label.WorkOrderBillingTypeBillable}
   )
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(

	NOT(ISBLANK([SVMXC__Service_Order__c].SVMXC__Component__c)),
	NOT(ISBLANK([SVMXC__Service_Order__c].SVMXC__Component__c.SVMXC__Product__c )),
	NOT(ISBLANK([SVMXC__Service_Order__c].SVMXC__Component__c.SVMXC__Product__c.Regulatory_Type__c)),
	ISCHANGED([SVMXC__Service_Order__c].SVMXC__Order_Status__c),
	PRIORVALUE([SVMXC__Service_Order__c].SVMXC__Order_Status__c)&lt;&gt;$Label.WorkOrderStatusCompleted,
	TEXT([SVMXC__Service_Order__c].SVMXC__Order_Status__c)= $Label.WorkOrderStatusCompleted,
	OR(
		AND(NOT(ISBLANK([SVMXC__Service_Order__c].SVMXC__Case__c )), TEXT([SVMXC__Service_Order__c].SVMXC__Case__c.Sub_Type__c) &lt;&gt; $Label.CaseSubTypeComplaint),
