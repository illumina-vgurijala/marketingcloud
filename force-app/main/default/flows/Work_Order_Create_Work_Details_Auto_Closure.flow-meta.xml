NOT(TEXT([SVMXC__Service_Order__c].Parts_Order_Status__c)=$Label.PartsOrderStatusInProgress)
	)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_7</name>
        <dataType>Boolean</dataType>
        <expression>
AND( 
     OR(
              ISCHANGED({!myVariable_current.SVMXC__Order_Status__c}), 
              ISCHANGED({!myVariable_current.Parts_Order_Status__c}),
              ISCHANGED({!myVariable_current.Total_Open_Work_Details__c}),
