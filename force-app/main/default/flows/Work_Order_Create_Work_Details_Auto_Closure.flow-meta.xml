	),
	CONTAINS([SVMXC__Service_Order__c].SVMXC__Component__c.SVMXC__Product__c.Regulatory_Type__c,  $Label.IPRegulatoryTypeDX)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_4</name>
        <dataType>Boolean</dataType>
        <expression>AND(

	NOT(ISBLANK({!myVariable_current.SVMXC__Component__c})),
	NOT(ISBLANK({!myVariable_current.SVMXC__Component__r.SVMXC__Product__c} )),
	NOT(ISBLANK({!myVariable_current.SVMXC__Component__r.SVMXC__Product__r.Regulatory_Type__c})),
	ISCHANGED({!myVariable_current.SVMXC__Order_Status__c}),
	PRIORVALUE({!myVariable_current.SVMXC__Order_Status__c})&lt;&gt;{!$Label.WorkOrderStatusCompleted},
	TEXT({!myVariable_current.SVMXC__Order_Status__c})= {!$Label.WorkOrderStatusCompleted},
	OR(
		AND(NOT(ISBLANK({!myVariable_current.SVMXC__Case__c} )), TEXT({!myVariable_current.SVMXC__Case__r.Sub_Type__c}) &lt;&gt; {!$Label.CaseSubTypeComplaint}),
		ISBLANK({!myVariable_current.SVMXC__Case__c} )
	),
	CONTAINS({!myVariable_current.SVMXC__Component__r.SVMXC__Product__r.Regulatory_Type__c},  {!$Label.IPRegulatoryTypeDX})
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>
AND( 
     OR(
              ISCHANGED([SVMXC__Service_Order__c].SVMXC__Order_Status__c), 
              ISCHANGED([SVMXC__Service_Order__c].Parts_Order_Status__c),
              ISCHANGED([SVMXC__Service_Order__c].Total_Open_Work_Details__c),
