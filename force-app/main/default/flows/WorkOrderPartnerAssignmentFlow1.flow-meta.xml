            <conditions>
                <leftValueReference>$Record.RecordType.Name</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>$Label.RecordTypeEstimates</elementReference>
                </rightValue>
            </conditions>
            <conditions>
