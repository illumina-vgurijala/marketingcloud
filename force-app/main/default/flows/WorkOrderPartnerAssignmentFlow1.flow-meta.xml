            <conditions>
                <leftValueReference>$Record__Prior.SVMXC__Order_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Label.PendingReviewOrderStatus</elementReference>
                </rightValue>
            </conditions>
