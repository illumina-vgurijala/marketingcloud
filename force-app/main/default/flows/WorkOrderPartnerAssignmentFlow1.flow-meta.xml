                    <elementReference>$Label.PendingReviewOrderStatus</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Work_Detail_Count__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
