    <decisions>
        <name>Is_Instrument_Found</name>
        <label>Is Instrument Found</label>
        <locationX>352</locationX>
        <locationY>293</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Instrument_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varInstrumentId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue/>
                </rightValue>
            </conditions>
            <connector>
