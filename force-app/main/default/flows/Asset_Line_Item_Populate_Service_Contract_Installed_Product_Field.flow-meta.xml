            </connector>
            <label>Instrument Found</label>
        </rules>
        <rules>
            <name>Instrument_Not_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varInstrumentId</leftValueReference>
