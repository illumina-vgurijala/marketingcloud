    </formulas>
    <formulas>
        <name>formula_1_myRule_3_A1_1918412028</name>
        <dataType>String</dataType>
        <expression>IF(
    NOT(ISBLANK({!Get_MDR_Records.Ultimate_Consignee__c} )),
    TEXT({!Get_MDR_Records.Ultimate_Consignee__r.BillingCountryCode}),
    &apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>formula_3_Formula_8820705242</name>
        <dataType>String</dataType>
        <expression>{!$Label.AccountProspect}</expression>
    </formulas>
    <formulas>
        <name>formula_4_Formula_8345138953</name>
        <dataType>String</dataType>
        <expression>{!$Label.AccountRecordTypeDeveloperNameChannelPartnerProspect}</expression>
    </formulas>
    <formulas>
        <name>formula_5_Formula_2348262735</name>
        <dataType>String</dataType>
        <expression>{!$Label.MasterDataRequestUpgradeToChannelPartner}</expression>
    </formulas>
    <formulas>
        <name>formula_TRUE_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>OR(
    AND(
         ISNEW(),
         ISBLANK({!$Record.Ultimate_Consignee__c})
    ),
    AND(
        ISCHANGED({!$Record.Ultimate_Consignee__c}),
        ISBLANK({!$Record.Ultimate_Consignee__c})
    ),
AND(
        ISCHANGED({!$Record.Trigger_Email__c}),
          {!$Record.Trigger_Email__c} = true
)
)</expression>
    </formulas>
    <formulas>
        <name>formula_TRUE_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>OR(
         ISNEW(),
    AND(
        ISCHANGED({!$Record.Ultimate_Consignee__c}),
        NOT(ISBLANK({!$Record.Ultimate_Consignee__c}))
    )
)</expression>
    </formulas>
