        <rules>
            <name>Check_for_Opp_Record_Type</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>GetOpportunity.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>DFS_Record_Type.DeveloperName</elementReference>
                </rightValue>
            </conditions>
