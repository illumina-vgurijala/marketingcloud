                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>IDFS_Record_Type.DeveloperName</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>GetOpportunity.Product_Interest__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>Relocation</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>EmailScreen</targetReference>
            </connector>
            <label>Check for Opp Record Type</label>
        </rules>
