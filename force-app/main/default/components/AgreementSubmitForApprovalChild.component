                                    </td>
                                </tr>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!IF(AND(objAgreement.Reason_for_Non_Standard_Freight__c != null, objAgreement.Reason_for_Non_Standard_Freight__c != ''), true, false)}">
