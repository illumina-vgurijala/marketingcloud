<apex:component controller="ApttusAgrementApprovalController" access="global">
    <style>
        .legendBoxWhite{
            height:12px;
            width:15px; 
            border:1px solid;
            background-color:white;
        }
        .legendBoxHighlight{
            height:12px;
            width:15px; 
            border:1px solid;
            background-color:#FFFFE0;
        }
        .legendTextNormal{
            height:12px; 
            width:65px; 
            font-size: 8px; 
            border:none;
            color : black;
        }
        .legendTextGray{
            height:12px; 
            width:75px; 
            font-size: 8px; 
            border:none;
            color : #808080;
        }
        .warningRed{
            font-size: 17px;
            color : red;
        }
        .dgColumnHeader{
            text-align: center; 
            font-weight: bold; 
            word-wrap: break-word !important;
        }
        .dgColumnOutput{
            text-align: center; 
            word-wrap: break-word !important;
        }
    </style>
    <apex:attribute name="AgreementId" type="String" description="Id of the Agreement" assignTo="{!strAgreementId}"/>
    <apex:attribute name="ApprovalRequestObject" type="Apttus_Approval__Approval_Request__c" description="Approval Request Object" assignTo="{!objApprovalRequest}"/>
    <apex:attribute name="EmailType" type="String" description="Type of email notification" assignTo="{!strEmailType}"/> 
    <apex:attribute name="AgreementRecords" type="Apttus__APTS_Agreement__c[]" description="list of agreement" assignTo="{!lstAgreement}"/> 
    {!LoadAgreementData}
    <apex:outputPanel id="emailMessage" rendered="{!channelPartnerLineItemData}">
        <apex:outputPanel id="NonReminderContent1" rendered="{!IF((strEmailType == 'APPROVALEMAIL'), true, false)}"> 
            <p><apex:outputText value="{!lstNonNogotiatedAgreements[0].Apttus__Agreement_Number__c+'.'+TEXT(lstNonNogotiatedAgreements[0].Apttus__Version_Number__c)}"/> 
                for {!lstNonNogotiatedAgreements[0].Name} with {!lstNonNogotiatedAgreements[0].Apttus__Account__r.Name} 
                has been submitted for approval by Global Channel Operations for {!lstNonNogotiatedAgreements[0].Apttus__Account__r.Name}.
                Please review the details below to approve or reject this request.</p>
        </apex:outputPanel>
        <apex:outputPanel id="HideIfRecall1" rendered="{!IF((strEmailType == 'RECALL'), false, true)}"><!--DCP-37233 Added to hide section in Recall Email--> 
             Reply to this email with the word APPROVE or REJECT in the first line of the email message.<br/>
            <br/>
            <a href="{!strApproveRejectURL}" target="_top">Approve/Reject</a><br/>
            <a href="{!strCommentURL}" target="_top">Add Comment</a>
            <p>If replying via email you can also add comments on the second line followed by a BLANK line. The comments will be stored with the approval request.</p><br/>
        </apex:outputPanel>
    </apex:outputPanel>
    
    <apex:outputPanel id="emailMessageNonChannelPartner" rendered="{! !channelPartnerLineItemData}">
        <apex:outputPanel id="NonReminderContent2" rendered="{!IF((strEmailType == 'APPROVALEMAIL'), true, false)}"> 
            <p>A {!lstNonNogotiatedAgreements[0].recordType.Name}  has been submited for {!lstNonNogotiatedAgreements[0].Apttus__Account__r.Name}.Please review the details below to approve or reject this request.</p>
        </apex:outputPanel>
        <apex:outputPanel id="HideIfRecall2" rendered="{!IF((strEmailType == 'RECALL'), false, true)}"><!--DCP-37233 Added to hide section in Recall Email-->
            Reply to this email with the word APPROVE, APPROVED, YES, REJECT, REJECTED, or NO in the first line of the email message.<br/>
            <br/>
            <a href="{!strApproveRejectURL}" target="_top">Approve/Reject</a><br/>
            <a href="{!strCommentURL}" target="_top">Add Comment</a>
            <p>If replying via email you can also add comments on the second line followed by a BLANK line. The comments will be stored with the approval request.</p>
        </apex:outputPanel>
    </apex:outputPanel>
     
    <apex:outputPanel id="CLickHere" rendered="{!IF((strEmailType != 'RECALL'), true, false)}"> 
        To review this Agreement directly in SFDC, please click <a href="{!HTMLENCODE(strAgreementUrl)}">here</a>.
    </apex:outputPanel>
        <br/>

    <apex:outputPanel id="CLickHereTableau" rendered="{!IF((strEmailType != 'RECALL'), true, false)}"> 
        To review analytics data for this Agreement in Tableau, please click <a href="{!HTMLENCODE(strTableauLink)}">here</a>.
    </apex:outputPanel>
        <br/><br/>

