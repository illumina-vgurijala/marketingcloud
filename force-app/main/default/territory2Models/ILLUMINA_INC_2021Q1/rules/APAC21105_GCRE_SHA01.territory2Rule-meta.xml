<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>(1 AND 2 AND 3 AND 4 AND 6) OR (1 AND 5)</booleanFilter>
    <name>APAC21105 - GCRE_SHA01</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>CN</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingCity</field>
        <operation>contains</operation>
        <value>Suzhou,Shanghai</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Customer_Type__c</field>
        <operation>equals</operation>
        <value>Academic</value>
    </ruleItems>
    <ruleItems>
        <field>Account.RecordTypeId</field>
        <operation>notEqual</operation>
        <value>Illumina Internal</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>contains</operation>
        <value>Suzhou Public,Suzhou Center,Suzhou Wujiang</value>
    </ruleItems>
    <ruleItems>
        <field>Account.ERP_Customer_Id__c</field>
        <operation>notContain</operation>
        <value>4000029793,6000025578,4000001729,4000013435,4000015104,4000001696</value>
    </ruleItems>
</Territory2Rule>
