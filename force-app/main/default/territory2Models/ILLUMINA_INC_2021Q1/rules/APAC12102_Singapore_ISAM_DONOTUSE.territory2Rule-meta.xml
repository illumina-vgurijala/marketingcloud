<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <booleanFilter>1 AND 2 AND 3</booleanFilter>
    <name>APAC12102 - Singapore ISAM (DONOTUSE)</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>SG</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Tier__c</field>
        <operation>equals</operation>
        <value>Tier 3</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>notContain</operation>
        <value>NovogeneAIT,Macrogen,Medgenome,SCELSE,Genome Institute Of Singapore,Eurofins</value>
    </ruleItems>
</Territory2Rule>
