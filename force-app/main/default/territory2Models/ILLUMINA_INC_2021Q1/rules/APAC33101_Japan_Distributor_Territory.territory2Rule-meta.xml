<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>(1 AND 2) OR (1 AND 3)</booleanFilter>
    <name>APAC33101 - Japan Distributor Territory</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>JP</value>
    </ruleItems>
    <ruleItems>
        <field>Account.RecordTypeId</field>
        <operation>equals</operation>
        <value>Indirect Partner,Channel Partner Prospect</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Account_Group__c</field>
        <operation>contains</operation>
        <value>Distributor</value>
    </ruleItems>
</Territory2Rule>
