<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>1 AND ((2 AND 3) OR 4 OR (5 AND 6))</booleanFilter>
    <name>EMEA11208 - Germany S Clinical</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>DE</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingCity</field>
        <operation>equals</operation>
        <value>Bamberg,Coburg,Erlangen,Landau in der Pfalz,Neustadt an der Aisch,Schweinfurt,Volkach,Würzburg,Darmstadt</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>notContain</operation>
        <value>Merck</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>contains</operation>
        <value>Universität Tübingen,Universitätsklinikum Tübingen</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingCity</field>
        <operation>equals</operation>
        <value>Karlsruhe,Heilbronn,Ludwigsburg,Pforzheim,Reutlingen,Schwäbisch Gmünd,Schwäbisch Hall,Crailsheim,Sindelfingen,Böblingen,Ettlingen,Fellbach,Herrenberg,Bruchsal,Eggenstein-Leopoldshafen,Kieselbronn,Kirchheim unter Teck,Leonberg,Mockmuhl,Rottenburg,Sindelfingen,Sinsheim,Löwenstein</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Clinical_Type__c</field>
        <operation>notEqual</operation>
        <value>Non Clinical</value>
    </ruleItems>
</Territory2Rule>
