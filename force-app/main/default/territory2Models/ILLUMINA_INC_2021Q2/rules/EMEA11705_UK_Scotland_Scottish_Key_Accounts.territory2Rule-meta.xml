<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>1 AND 2 AND 3</booleanFilter>
    <name>EMEA11705 - UK Scotland &amp; Scottish Key Accounts</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>GB</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingPostalCode</field>
        <operation>startsWith</operation>
        <value>AB,DD,DG,EH,FK,G1,G2,G3,G4,G5,G6,G7,G8,G9,HS,IV,KA,KW,KY,ML,PA,PH,TD,ZE</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>notContain</operation>
        <value>NHS Grampian,NHS Greater Glasgow and Clyde,NHS Lothian,NHS National Services Scotland,NHS Tayside,University Of Dundee,University of Aberdeen,University of Glasgow</value>
    </ruleItems>
</Territory2Rule>
