<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>(1 AND 2 AND 3 AND 4 AND 5 AND 8 AND 9) OR (1 AND 2 AND 6 AND 7 AND 8 AND 9)</booleanFilter>
    <name>APAC14105 - Korea Research 1</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>KR</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Clinical_Type__c</field>
        <operation>equals</operation>
        <value>Non Clinical,Clinical R&amp;D</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingStateCode</field>
        <operation>contains</operation>
        <value>13</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingPostalCode</field>
        <operation>greaterOrEqual</operation>
        <value>05200</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingPostalCode</field>
        <operation>lessOrEqual</operation>
        <value>09000</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingStateCode</field>
        <operation>contains</operation>
        <value>09</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingCity</field>
        <operation>contains</operation>
        <value>Suwon,Anyang,Gwangmyeong,Ansan,Gwacheon,Osan,Siheung,Gunpo,Uiwang,Hwaseong,Gwangju,Yangju</value>
    </ruleItems>
    <ruleItems>
        <field>Account.RecordTypeId</field>
        <operation>notEqual</operation>
        <value>Indirect Partner,Channel Partner Prospect</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>notContain</operation>
        <value>Macrogen,DNA Link,Theragen Etex,Theragen Bio,DCGEN,NGeneBio,Geninus</value>
    </ruleItems>
</Territory2Rule>
