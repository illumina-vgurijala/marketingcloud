<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>1 AND 2 AND 3</booleanFilter>
    <name>EMEA11709 - UK Cambridge</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>GB</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingPostalCode</field>
        <operation>startsWith</operation>
        <value>CB</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>contains</operation>
        <value>AstraZeneca,Cambridge University Hospitals NHS Foundation Trust,Cancer Research UK,University of Cambridge,Wellcome Trust Centre</value>
    </ruleItems>
</Territory2Rule>
