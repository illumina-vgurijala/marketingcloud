<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>(1 AND 2 AND 3 AND 4 AND 5 AND 8) OR (1 AND 2 AND 6 AND 7 AND 8) OR (1 AND 2 AND 10) OR (1 AND 9)</booleanFilter>
    <name>APAC14101 - Korea Clinical 1</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>KR</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Clinical_Type__c</field>
        <operation>equals</operation>
        <value>Clinical</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingStateCode</field>
        <operation>contains</operation>
        <value>13</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingPostalCode</field>
        <operation>greaterOrEqual</operation>
        <value>05200</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingPostalCode</field>
        <operation>lessOrEqual</operation>
        <value>09000</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingStateCode</field>
        <operation>contains</operation>
        <value>09</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingCity</field>
        <operation>contains</operation>
        <value>Suwon,Seongnam,Bucheon,Anyang,Gwangmyeong,Pyeongtaek,Ansan,Gwacheon,Osan,Siheung,Gunpo,Uiwang,Hanam,Yongin,Icheon,Anseong,Hwaseong,Gwangju,Yangju</value>
    </ruleItems>
    <ruleItems>
        <field>Account.RecordTypeId</field>
        <operation>notEqual</operation>
        <value>Indirect Partner,Channel Partner Prospect</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>contains</operation>
        <value>Asan Medical Center,Asan Medical Ctr,Samsung Medical Center,Samsung Medical Ctr,Samsung Hosp,The Catholic University of Korea,Catholic University of Korea,The Catholic Univ of Korea,Catholic Univ of Korea</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>startsWith</operation>
        <value>Hallym Univ,KyungHee Univ,Kyung Hee Univ</value>
    </ruleItems>
</Territory2Rule>
