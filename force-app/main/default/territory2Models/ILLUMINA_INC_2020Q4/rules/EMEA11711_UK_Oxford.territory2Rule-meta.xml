<?xml version="1.0" encoding="UTF-8"?>
<Territory2Rule xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <booleanFilter>1 AND 2 AND 3</booleanFilter>
    <name>EMEA11711 - UK Oxford</name>
    <objectType>Account</objectType>
    <ruleItems>
        <field>Account.BillingCountryCode</field>
        <operation>contains</operation>
        <value>GB</value>
    </ruleItems>
    <ruleItems>
        <field>Account.BillingPostalCode</field>
        <operation>startsWith</operation>
        <value>OX</value>
    </ruleItems>
    <ruleItems>
        <field>Account.Name</field>
        <operation>equals</operation>
        <value>University of Oxford,Wellcome Trust for Human Genetics,Novo Nordisk Ltd,Oxford University Hospitals NHS Foundation Trust,Churchill Hospital</value>
    </ruleItems>
</Territory2Rule>
