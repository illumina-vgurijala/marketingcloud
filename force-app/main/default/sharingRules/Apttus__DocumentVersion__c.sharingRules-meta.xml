<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Agreements_Docs_With_GCP</fullName>
        <accessLevel>Edit</accessLevel>
        <label>Agreements Docs With GCP</label>
        <sharedTo>
            <group>Customer_Agreements_GCP_team</group>
        </sharedTo>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Agreement_Record_Type__c</field>
            <operation>equals</operation>
            <value>Channel_Partner_Agreement</value>
        </criteriaItems>
        <criteriaItems>
            <field>Agreement_Record_Type__c</field>
            <operation>equals</operation>
            <value>Master_Customer_Agreement</value>
        </criteriaItems>
    </sharingCriteriaRules>
