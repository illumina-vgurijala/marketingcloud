<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Associated_Code_Sharing_All_Service_R_W</fullName>
        <accessLevel>Edit</accessLevel>
        <description>To share the Associated Code with all Service users with Read/Write Access</description>
        <label>Associated Code Sharing - All Service : R/W</label>
        <sharedTo>
            <roleAndSubordinatesInternal>Service</roleAndSubordinatesInternal>
        </sharedTo>
        <criteriaItems>
            <field>CreatedById</field>
            <operation>notEqual</operation>
            <value>NULL</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Share_Associated_Codes_to_TPSP</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share the Associated Codes to TPSP Users.</description>
        <label>Share Associated Codes to TPSP</label>
        <sharedTo>
            <roleAndSubordinatesInternal>Field_Service_Third_Party</roleAndSubordinatesInternal>
        </sharedTo>
        <criteriaItems>
            <field>CreatedById</field>
            <operation>notEqual</operation>
            <value/>
        </criteriaItems>
    </sharingCriteriaRules>
</SharingRules>
