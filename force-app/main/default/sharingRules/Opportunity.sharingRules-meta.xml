    <sharingCriteriaRules>
        <fullName>Service_AMR_Opportunity_Read_Only</fullName>
        <accessLevel>Read</accessLevel>
        <description>INC0195462: Enabling AMR Service Directors/Managers to have read only access to AMR Opportunities.</description>
        <label>Service AMR Opportunity Read Only</label>
        <sharedTo>
            <group>ServiceAMROpportunityAndQuotesReadOnly</group>
        </sharedTo>
        <booleanFilter>1 OR 2</booleanFilter>
        <criteriaItems>
            <field>Sold_To_Account_Region__c</field>
            <operation>equals</operation>
            <value>AMR</value>
        </criteriaItems>
        <criteriaItems>
            <field>Ultimate_Consignee_Region__c</field>
            <operation>equals</operation>
            <value>AMR</value>
        </criteriaItems>
