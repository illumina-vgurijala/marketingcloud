<apex:page standardController="Contact" extensions="openq.BusinessObjectiveController">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection title="Business Objective" columns="1" collapsible="false">
                 <apex:pageBlockSectionItem >
                     <apex:outputLabel value="Enter Business Objective"></apex:outputLabel>
                     <apex:inputTextarea cols="70" rows="10" value="{!businessObjective}"/>
                 </apex:pageBlockSectionItem>
            </apex:pageBlockSection>
            <apex:pageBlockButtons location="bottom">
                <apex:commandButton value="Continue" action="{!continuePrintableBio}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
       
    </apex:form>
</apex:page>