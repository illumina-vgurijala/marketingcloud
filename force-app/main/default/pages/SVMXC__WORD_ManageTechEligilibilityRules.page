                        </apex:column>
                        
                        <apex:column width="50%">
                            <apex:facet name="header" >{!TAG010}</apex:facet>
                            <apex:outputText value="{!Process1.SVMXC__Description__c}" rendered="{!if(Process1.SVMXC__Select__c==false,true,false)}"/>
