                                    <apex:selectOptions value="{!lstOperatorSelectLst}"/>
                                </apex:selectList>
                                 <apex:selectList value="{!wrapobj.objConf.SVMXC__Operator__c}"  size="1" multiselect="false" style="font-Size:12px" rendered="{!wrapobj.IsRenderpicklist}">
