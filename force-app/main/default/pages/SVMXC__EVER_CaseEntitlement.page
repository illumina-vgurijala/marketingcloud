        <script>
            function goToSelectedID(seledtId)
            {
                
                try
                {
                    if(seledtId.length == 0)
                        seledtId = 'null';
                    //var url = window.location.href; //Commented for security fix SECSCAN-803
                    var url = '{!JSINHTMLENCODE($CurrentPage.URL)}'; //Added for security fix SECSCAN-803
                    var arr = url.split('/apex');
                    var result = arr[0]+'/'+seledtId;
                    window.open(result, '_blank');
                    window.focus();
                }
                catch(ex)
                {
                    alert(ex);
                }
            }
