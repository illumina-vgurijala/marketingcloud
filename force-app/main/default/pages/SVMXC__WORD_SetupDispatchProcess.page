        function dhtmlxSlider (elem, obj){
            obj.parent = elem;
            return new dhtmlXSlider(obj);
        }
                
        // XSS security issue, 
        function htmlspecialchars(str) {
            var map = {
                "&": "&amp;",
                "<": "&lt;",
                ">": "&gt;",
                "\"": "&quot;",
                "'": "&#39;" // ' -> &apos; for XML only
            };
            return str.replace(/[&<>"']/g, function(m) { return map[m]; });
