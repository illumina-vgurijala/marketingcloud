<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Download_Cycle_Count_to_Mobile__c</fullName>
    <description>Field set to 1 if the related work order is downloaded to mobile or the owner is logged in user</description>
    <externalId>false</externalId>
    <formula>if(OR(Work_Order__r.Download_Work_Order_to_Mobile__c=1, OwnerId = $User.Id ),1,0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Download Cycle Count to Mobile</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
