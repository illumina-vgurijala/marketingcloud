<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Decontamination_Required__c</fullName>
    <description>To Populate the Decontamination Required of the associated Product. 
Automatically Populated if Product is associated to the Parts Order Line.</description>
    <externalId>false</externalId>
    <formula>AND( SVMXC__Product__r.Decontamination_Required__c , NOT(ISPICKVAL(Return_Reason__c, &quot;Unused/Good Stock&quot;)) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Decontamination Required</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
