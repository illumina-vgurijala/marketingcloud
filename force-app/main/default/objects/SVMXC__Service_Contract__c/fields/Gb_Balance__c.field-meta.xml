<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Gb_Balance__c</fullName>
    <description>Remaining gigabases if Gb Consumed is not blank.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(Gb_Consumed__c)),Gb_Quota__c - Gb_Consumed__c, Gb_Quota__c)</formula>
    <label>Gb Balance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
