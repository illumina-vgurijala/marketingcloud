<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SVMXC__Utilization__c</fullName>
    <deprecated>false</deprecated>
    <description>Holds the utilization of the technician for the day.</description>
    <externalId>false</externalId>
    <formula>IF((SVMXC__Available_Time__c &gt; 0),
(SVMXC__Productive_Time__c / SVMXC__Available_Time__c) * 100,
0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Holds the utilization of the technician for the day.</inlineHelpText>
    <label>Utilization %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
