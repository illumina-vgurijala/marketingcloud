<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Product_Blocked__c</fullName>
    <description>To block the Part Order Line if the Product&apos;s Cross Plant Material Status value contains Y6, Y7, Y8, Z0, Z1, Z2</description>
    <externalId>false</externalId>
    <formula>AND(
NOT(ISBLANK(SVMXC__Product__c)), SVMXC__Product__r.Is_Product_Blocked__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Product Blocked</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
