<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Installed_Product_Date__c</fullName>
    <description>Automatically checked if the created date is greater than date installed.</description>
    <externalId>false</externalId>
    <formula>IF( CreatedDate   &gt; DATETIMEVALUE( SVMXC__Date_Installed__c ) , true,false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Installed Product Date</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
