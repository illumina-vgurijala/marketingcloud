<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expiration_Status__c</fullName>
    <description>To Capture Red Color after Expiry of Installed Product
Automatically Populate if Expiration Date is less than Today</description>
    <externalId>false</externalId>
    <formula>IF(No_of_Days_till_Expiry__c &lt; 0,IMAGE(&quot;/img/samples/color_red.gif&quot;, &quot;red&quot;, 30, 30),&apos;&apos;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Expiration Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
