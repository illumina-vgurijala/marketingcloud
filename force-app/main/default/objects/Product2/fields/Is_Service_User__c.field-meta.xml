<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Service_User__c</fullName>
    <description>Is Service User will be true if logged user is service user.</description>
    <externalId>false</externalId>
    <formula>OR(
 $Profile.Name ==$Label.Channel_Partner_Profile_Name,
$Profile.Name ==$Label.ProfileChannelPartnerService,
$Profile.Name ==$Label.ProfileChannelPartnerSalesandReadOnlyService,
$Profile.Name ==$Label.ProfileChannelPartnerSalesandReadOnlyServiceLog,
$Profile.Name ==$Label.ProfileCustomerService,
$Profile.Name ==$Label.ProfileDataSteward,
$Profile.Name ==$Label.ProfileFastTrackServices,
$Profile.Name ==$Label.ProfileFieldCompliance,
$Profile.Name ==$Label.ProfileFieldService,
$Profile.Name ==$Label.ProfileProductSupport,
$Profile.Name ==$Label.ProfileProfessionalServices,
$Profile.Name ==$Label.ProfileQualityAssurance,
$Profile.Name ==$Label.ProfileServiceCenter,
$Profile.Name ==$Label.ProfileServiceLogisticsUsers,
$Profile.Name ==$Label.ProfileServiceReadOnly,
$Profile.Name ==$Label.ProfileSystemAdmin,
$Profile.Name ==$Label.ProfileTechnicalSupport,
$Profile.Name ==$Label.ProfileConcierge,
$Permission.SS_Service_Ops_Edit_Custom_Perm,
$Permission.SS_Service_objects_Read_Only_Custom_Perm
)</formula>
