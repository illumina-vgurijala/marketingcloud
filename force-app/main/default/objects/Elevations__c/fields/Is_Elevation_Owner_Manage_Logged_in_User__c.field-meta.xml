<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Elevation_Owner_Manage_Logged_in_User__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>Required for list view for identifying whether the logged in user is Elevation Owner or Elevation Manager on all open elevations</description>
    <externalId>false</externalId>
    <formula>OR( Elevation_Owner__c = $User.Id , Elevation_Manager__c = $User.Id )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Elevation Owner/Manage Logged in User</label>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
