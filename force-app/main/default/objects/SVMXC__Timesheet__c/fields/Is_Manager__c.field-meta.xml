<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Manager__c</fullName>
    <description>Field to check if logged in user is user&apos;s manager. It is a back end field which will be use to create list view.</description>
    <externalId>false</externalId>
    <formula>IF(SVMXC__User__r.ManagerId = $User.Id ,True,False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Manager?</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
