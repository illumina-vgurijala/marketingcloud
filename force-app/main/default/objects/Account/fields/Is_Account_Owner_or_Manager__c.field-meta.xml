<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Account_Owner_or_Manager__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>Formula field used in report to check if the logged in user is Account owner or Manager of those Account Owners
PM-41</description>
    <externalId>false</externalId>
    <formula>(Owner.ManagerId = $User.Id|| OwnerId = $User.Id)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Account Owner or Manager?</label>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
