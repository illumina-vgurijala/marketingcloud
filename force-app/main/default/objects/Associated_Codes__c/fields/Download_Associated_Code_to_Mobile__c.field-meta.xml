<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Download_Associated_Code_to_Mobile__c</fullName>
    <description>Formula field to mark download associated code to mobile true if associated case or work order is having this field value to true.</description>
    <externalId>false</externalId>
    <formula>IF(( Case__r.Download_Case_To_Mobile__c == 1)||( Work_Order__r.Download_Work_Order_to_Mobile__c == 1), 1, 0)</formula>
    <label>Download Associated Code to Mobile</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
