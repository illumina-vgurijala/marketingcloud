<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Preferred_Language_Keyword__c</fullName>
    <description>Field to map Preferred Language keyword based on related Contact's Preferred Language</description>
    <externalId>false</externalId>
    <formula>CASE(Contact__r.Preferred_Language__c, &apos;English&apos;, &apos;us_en&apos;,	
                                        &apos;French&apos;,&apos;fr&apos;,	
                                        &apos;Chinese&apos;,&apos;zh_CN&apos;,	
                                        &apos;German&apos;,&apos;de&apos;,	
                                        &apos;Italian&apos;,&apos;it&apos;,	
                                        &apos;Spanish&apos;,&apos;es&apos;,	
                                        &apos;Korean&apos;,&apos;ko&apos;,	
                                        &apos;Japanese&apos;, &apos;ja&apos;,	
                                         &apos;Other&apos;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Preferred Language Keyword</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
