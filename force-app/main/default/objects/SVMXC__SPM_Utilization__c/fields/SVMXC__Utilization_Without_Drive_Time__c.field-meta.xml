<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SVMXC__Utilization_Without_Drive_Time__c</fullName>
    <deprecated>false</deprecated>
    <description>Computes the productive Utilization Time without Drive-Time using the Productive-Time, Available-Time</description>
    <externalId>false</externalId>
    <formula>IF(SVMXC__Available_Time__c &gt; 0, ((SVMXC__Productive_Time__c / SVMXC__Available_Time__c ) * 100), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Utilization % (Without Drive Time)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
