<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SVMXC__Available_Time__c</fullName>
    <deprecated>false</deprecated>
    <description>Holds the difference of minutes between working-minutes and break-minutes</description>
    <externalId>false</externalId>
    <formula>SVMXC__Working_Minutes__c  -  (SVMXC__Break_Minutes__c +  SVMXC__Off_Time__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Available Time (In Mins)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
