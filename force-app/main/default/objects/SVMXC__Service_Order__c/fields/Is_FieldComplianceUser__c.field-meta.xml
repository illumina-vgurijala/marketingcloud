<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_FieldComplianceUser__c</fullName>
    <description>Automatically checked if the user is Field Compliance user or user is having Field Compliance Review permission.</description>
    <externalId>false</externalId>
    <formula>OR( $Permission.SS_Field_Compliance_Review , $Profile.Name =&apos;Field Compliance&apos;)</formula>
    <label>Is FieldComplianceUser</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
