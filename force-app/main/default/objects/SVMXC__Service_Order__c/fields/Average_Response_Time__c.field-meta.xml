<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Average_Response_Time__c</fullName>
    <description>To calculate the average time between initial response date to scheduled on date</description>
    <externalId>false</externalId>
    <formula>((WO_Scheduled_On__c -  SVMXC__Actual_Initial_Response__c)*24)*60</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Average Response Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
