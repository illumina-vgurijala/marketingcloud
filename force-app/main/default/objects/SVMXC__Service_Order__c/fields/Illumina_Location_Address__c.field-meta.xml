<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Illumina_Location_Address__c</fullName>
    <description>To Populate the Illumina Location Address from the Illumina Address Field
Automatically Populated if Illumina Address is associated to the Work Order.</description>
    <externalId>false</externalId>
    <formula>Illumina_Address__r.Name+&quot;&lt;/br&gt;&quot;+
Illumina_Address__r.SVMXC__Street__c+&quot;&lt;/br&gt;&quot;+
Illumina_Address__r.SVMXC__City__c +&quot;&lt;/br&gt;&quot;+
Illumina_Address__r.SVMXC__Zip__c+&quot;&lt;/br&gt;&quot;+
TEXT(Illumina_Address__r.SVMXC__Country__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Illumina Location Address</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
