<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Template_Quantity__c</fullName>
    <description>Stores text value for quantity so that decimals are not displayed on template.</description>
    <externalId>false</externalId>
    <formula>TEXT ( ROUND ( Apttus_QPConfig__Quantity2__c , 2 ) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Template Quantity</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
