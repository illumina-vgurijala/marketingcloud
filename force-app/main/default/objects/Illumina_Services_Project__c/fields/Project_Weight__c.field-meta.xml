<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_Weight__c</fullName>
    <description>Field to calculate Project Weight</description>
    <externalId>false</externalId>
    <formula>VALUE(TEXT(Visibility__c)) + VALUE(TEXT(Difficulty__c)) + VALUE(TEXT(Size__c)) +1</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Project Weight</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
