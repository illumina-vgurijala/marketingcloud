<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SVMXC__SVMXC_ValidateQuantityChange</fullName>
    <active>false</active>
    <description>Transfer quantity cannot be changed for existing records.</description>
    <errorConditionFormula>AND (!ISNEW(),  ISCHANGED( SVMXC__Quantity_Transferred2__c))</errorConditionFormula>
    <errorDisplayField>SVMXC__Quantity_Transferred2__c</errorDisplayField>
    <errorMessage>Once entered, transfer quantity cannot be changed.</errorMessage>
</ValidationRule>
