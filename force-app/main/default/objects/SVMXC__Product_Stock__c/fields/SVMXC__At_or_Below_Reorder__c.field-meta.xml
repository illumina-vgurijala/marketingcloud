<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SVMXC__At_or_Below_Reorder__c</fullName>
    <deprecated>false</deprecated>
    <description>Indicates whether the stockable item is at or below the defined reorder point and should now be ordered.</description>
    <externalId>false</externalId>
    <formula>IF(AND(SVMXC__Reorder_Level2__c &gt; 0, SVMXC__Reorder_Level2__c &gt;=  SVMXC__Quantity2__c), &quot;Yes&quot;, &quot;No&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>At or Below Reorder</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
