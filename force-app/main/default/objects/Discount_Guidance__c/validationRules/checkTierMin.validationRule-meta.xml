<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>checkTierMin</fullName>
    <active>true</active>
    <errorConditionFormula>Tier_Min__c &gt;= Tier_Max__c</errorConditionFormula>
    <errorMessage>Incorrect value. Tier Min should be always less than Tier Max.</errorMessage>
</ValidationRule>
