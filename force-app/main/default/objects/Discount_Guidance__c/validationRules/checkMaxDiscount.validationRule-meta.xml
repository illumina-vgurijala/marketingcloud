<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>checkMaxDiscount</fullName>
    <active>true</active>
    <errorConditionFormula>Max_Discount__c &lt; Median_Discount__c</errorConditionFormula>
    <errorMessage>Incorrect value. Max Discount should be always greater than or equal to Median Discount.</errorMessage>
</ValidationRule>
