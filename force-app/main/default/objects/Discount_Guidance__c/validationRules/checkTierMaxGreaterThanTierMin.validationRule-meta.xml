<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>checkTierMaxGreaterThanTierMin</fullName>
    <active>true</active>
    <errorConditionFormula>Tier_Max__c &lt;= Tier_Min__c</errorConditionFormula>
    <errorMessage>Incorrect value. Tier Max should be always greater than Tier Min.</errorMessage>
</ValidationRule>
