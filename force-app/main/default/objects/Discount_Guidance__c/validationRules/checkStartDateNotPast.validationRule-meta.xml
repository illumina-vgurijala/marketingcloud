<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>checkStartDateNotPast</fullName>
    <active>true</active>
    <errorConditionFormula>ISNEW() &amp;&amp; Start_Date__c &lt; TODAY()</errorConditionFormula>
    <errorMessage>Invalid date. The start date should not be in past at the time of upload.</errorMessage>
</ValidationRule>
