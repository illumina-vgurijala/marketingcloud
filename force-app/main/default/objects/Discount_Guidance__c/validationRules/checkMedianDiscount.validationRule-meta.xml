<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>checkMedianDiscount</fullName>
    <active>true</active>
    <errorConditionFormula>Median_Discount__c &gt; Max_Discount__c</errorConditionFormula>
    <errorMessage>Incorrect value. Median Discount should be always less than or equal to Max Discount.</errorMessage>
</ValidationRule>
