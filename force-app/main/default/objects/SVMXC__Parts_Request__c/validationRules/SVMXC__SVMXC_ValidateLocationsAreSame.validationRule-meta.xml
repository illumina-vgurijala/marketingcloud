<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SVMXC__SVMXC_ValidateLocationsAreSame</fullName>
    <active>true</active>
    <description>From and To locations cannot be the same.</description>
    <errorConditionFormula>(SVMXC__Requested_From__c =  SVMXC__Required_At_Location__c)</errorConditionFormula>
    <errorDisplayField>SVMXC__Requested_From__c</errorDisplayField>
    <errorMessage>Requested From location cannot be the same as Required At Location.</errorMessage>
</ValidationRule>
