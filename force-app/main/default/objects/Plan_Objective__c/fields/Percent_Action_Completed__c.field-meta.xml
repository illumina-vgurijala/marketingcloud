<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Percent_Action_Completed__c</fullName>
    <description>Percentage of number of Actions completed on a Plan</description>
    <externalId>false</externalId>
    <formula>IF( No_of_Actions__c == 0 ,0,(No_of_Completed_Actions__c / No_of_Actions__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Percentage of number of Actions completed on a Plan</inlineHelpText>
    <label>% Action Completed</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
