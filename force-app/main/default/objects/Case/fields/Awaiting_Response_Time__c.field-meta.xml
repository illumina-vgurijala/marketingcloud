<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Awaiting_Response_Time__c</fullName>
    <description>To calculate awaiting response time for the case</description>
    <externalId>false</externalId>
    <formula>(Awaiting_Response_Date__c  -  SVMXC__Actual_Initial_Response__c )*24</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Awaiting Response Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
