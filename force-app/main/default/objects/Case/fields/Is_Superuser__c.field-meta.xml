<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Superuser__c</fullName>
    <description>To check whether logged in user is super user</description>
    <externalId>false</externalId>
    <formula>OR($Permission.SS_ByPass_Case_Lock,$Profile.Name = $Label.ProfileQualityAssurance)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is Superuser</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
