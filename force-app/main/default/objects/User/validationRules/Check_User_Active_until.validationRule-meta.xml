<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_User_Active_until</fullName>
    <active>true</active>
    <description>Check User Active until &gt;= Today</description>
    <errorConditionFormula>User_Active_until__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>User_Active_until__c</errorDisplayField>
    <errorMessage>Please enter a future date.</errorMessage>
</ValidationRule>
