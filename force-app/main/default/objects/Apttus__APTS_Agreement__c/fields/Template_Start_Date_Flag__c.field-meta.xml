<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Template_Start_Date_Flag__c</fullName>
    <description>Formula to check if the start date is greater than Today</description>
    <externalId>false</externalId>
    <formula>If (
				Apttus__Contract_Start_Date__c &gt;=  Current_Date__c,
				true,
				false
				)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Template Start Date Flag</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
