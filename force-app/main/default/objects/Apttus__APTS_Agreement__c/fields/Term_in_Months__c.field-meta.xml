<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Term_in_Months__c</fullName>
    <description>Term of the Agreement in months</description>
    <externalId>false</externalId>
    <formula>VALUE(TEXT((YEAR(Apttus__Contract_End_Date__c)*12+MONTH(Apttus__Contract_End_Date__c))-
        (YEAR(Apttus__Contract_Start_Date__c)*12+MONTH(Apttus__Contract_Start_Date__c))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Term in Months</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
