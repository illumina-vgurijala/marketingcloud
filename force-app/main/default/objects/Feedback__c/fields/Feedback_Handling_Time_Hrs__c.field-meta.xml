<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Feedback_Handling_Time_Hrs__c</fullName>
    <description>To calculate time feedback opened from new to close</description>
    <externalId>false</externalId>
    <formula>FLOOR(( Feedback_Close_Date__c   -  CreatedDate ) * 24)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Feedback Handling Time (Hrs)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
