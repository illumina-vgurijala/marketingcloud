<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Address__c</fullName>
    <description>Formula field to get the full Billing Address from related account.</description>
    <externalId>false</externalId>
    <formula>SVMXC__Company__r.BillingStreet +&quot;, &quot;+
SVMXC__Company__r.BillingCity    + &quot;, &quot;+ 
SVMXC__Company__r.BillingState   +&quot;, &quot; + 
 SVMXC__Company__r.BillingPostalCode  + &quot;, &quot; + 
  SVMXC__Company__r.BillingCountry</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Account Address</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
